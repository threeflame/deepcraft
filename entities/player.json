{
  "format_version": "1.20.10",
  "minecraft:entity": {
    "description": {
      "identifier": "minecraft:player",
      "is_spawnable": false,
      "is_summonable": false,
      "is_experimental": false,
      "properties": {
        "status:arrow_damage": { 
          "type": "int", 
          "range": [-1, 300], 
          "default": -1
        } 
      },
      "scripts": {
        "animate": [ 
          "tag_attacking","tag_fire","tag_draw_bow"
        ]
      },
      "animations": {
        "tag_attacking": "controller.animation.tag_attacking",
        "tag_fire": "controller.animation.tag_fire",
        "tag_draw_bow": "controller.animation.tag_draw_bow"
      }
    },

    "component_groups": {
      "reset": {
        "minecraft:nameable": { "always_show":true },
        "minecraft:rideable": {
          "seat_count": 2,
          "family_types": [ "parrot_tame" ],
          "pull_in_entities": true,
          "seats": [
            { "position": [ 0.4, -0.2, -0.1 ], "min_rider_count": 0, "max_rider_count": 0, "lock_rider_rotation": 0 },
            { "position": [ -0.4, -0.2, -0.1 ], "min_rider_count": 1, "max_rider_count": 2, "lock_rider_rotation": 0 }
          ]
        }
      },
      "noname": { "minecraft:nameable": { "always_show":false } },
      "can_climb": { "minecraft:can_climb":{} },
      "is_stackable": { "minecraft:is_stackable":{} },
      "player_rideable": {
        "minecraft:rideable": {
          "seat_count": 1,
          "interact_text": "乗る",
          "family_types": [ "player" ],
          "seats": { "position": [ 0.0, -0.2, -0.35 ] }
        }
      },
      
      "attack_reset": { "minecraft:attack":{"damage":1} },
    
      "health_reset": { "minecraft:health": { "value": 99999, "max": 99999 } },
      
      "movement_reset": { "minecraft:movement":{"value":0.1} },
      "knockback_resistance_reset": { "minecraft:knockback_resistance": {"max": 0.0} },
      "scale_reset": { "minecraft:scale":{"value":1.0} },
      
      "attack0": {"minecraft:attack":{"damage":0}},
      "attack1": {"minecraft:attack":{"damage":1}},
      "movement0": {"minecraft:movement":{"value":0}},
      "movement1": {"minecraft:movement":{"value":0.01}},
      "knockback_resistance0": {"minecraft:knockback_resistance": {"max": 0}},
      "knockback_resistance100": {"minecraft:knockback_resistance": {"max": 1}},
      "scale0": {"minecraft:scale":{"value":0}},
      "scale100": {"minecraft:scale":{"value":1}},

      "minecraft:add_bad_omen": {
        "minecraft:spell_effects": {
          "add_effects": [ { "effect": "bad_omen", "duration": 6000, "visible": false, "display_on_screen_animation": true } ]
        },
        "minecraft:timer": {
          "time": [ 0.0, 0.0 ],
          "looping": false,
          "time_down_event": { "event": "minecraft:clear_add_bad_omen", "target": "self" }
        }
      },
      "minecraft:clear_bad_omen_spell_effect": { "minecraft:spell_effects": {} },
      "minecraft:raid_trigger": {
        "minecraft:raid_trigger": { "triggered_event": { "event": "minecraft:remove_raid_trigger", "target": "self" } },
        "minecraft:spell_effects": { "remove_effects": "bad_omen" }
      }
    },

    "components": {
      "minecraft:experience_reward": { "on_death": "Math.Min(query.player_level * 7, 100)" },
      "minecraft:type_family": { "family": [ "player" ] },
      "minecraft:is_hidden_when_invisible": {},
      "minecraft:loot": { "table": "loot_tables/empty.json" },
      "minecraft:collision_box": { "width": 0.6, "height": 1.8 },
      "minecraft:can_climb": {},
      "minecraft:movement": { "value": 0.1 },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          { "filters": { "test": "in_lava", "subject": "self", "operator": "==", "value": true }, "cause": "lava", "damage_per_tick": 4 }
        ]
      },
      "minecraft:attack": { "damage": 1 },
      "minecraft:exhaustion_values": {
        "heal": 6, "jump": 0.05, "sprint_jump": 0.2, "mine": 0.005, "attack": 0.1, "damage": 0.1, "walk": 0.0, "sprint": 0.1, "swim": 0.01
      },
      "minecraft:player.saturation": { "value": 5, "max": 20 },
      "minecraft:player.exhaustion": { "value": 0, "max": 20 },
      "minecraft:player.level": { "value": 0, "max": 24791 },
      "minecraft:player.experience": { "value": 0, "max": 1 },
      "minecraft:breathable": { "total_supply": 15, "suffocate_time": -1, "inhale_time": 3.75, "generates_bubbles": false },
      "minecraft:nameable": { "always_show": true, "allow_name_tag_renaming": false },
      "minecraft:physics": { "push_towards_closest_space": true },
      "minecraft:pushable": { "is_pushable": false, "is_pushable_by_piston": true },
      "minecraft:insomnia": { "days_until_insomnia": 3 },
      "minecraft:rideable": {
        "seat_count": 2,
        "family_types": [ "parrot_tame" ],
        "pull_in_entities": true,
        "seats": [
          { "position": [ 0.4, -0.2, -0.1 ], "min_rider_count": 0, "max_rider_count": 0, "lock_rider_rotation": 0 },
          { "position": [ -0.4, -0.2, -0.1 ], "min_rider_count": 1, "max_rider_count": 2, "lock_rider_rotation": 0 }
        ]
      },
      "minecraft:conditional_bandwidth_optimization": {},
      "minecraft:block_climber": {},
      "minecraft:environment_sensor": {
        "triggers": [
          {
            "filters": {
              "all_of": [
                { "test": "has_mob_effect", "subject": "self", "value": "bad_omen" },
                { "test": "is_in_village", "subject": "self", "value": true }
              ]
            },
            "event": "minecraft:trigger_raid"
          }
        ]
      },
      
      "minecraft:health": { "value": 99999, "max": 99999 }
    },

    "events": {
      "reset": { "add": {"component_groups": ["reset","can_climb"]}, "remove": {"component_groups": ["is_stackable"]} },
      "player_ride": {"add": {"component_groups": ["player_rideable"]}},
      "noname": {"add": {"component_groups": ["noname"]}},
      "can_not_climb": {"remove": {"component_groups": ["can_climb"]}},
      "stackable": {"add": {"component_groups": ["is_stackable"]}},
      "attack_reset": {"add": {"component_groups": ["attack_reset"]}},
      "health_reset": {"add": {"component_groups": ["health_reset"]}},
      "movement_reset": {"add": {"component_groups": ["movement_reset"]}},
      "knockback_resistance_reset": {"add": {"component_groups": ["knockback_resistance_reset"]}},
      "scale_reset": {"add": {"component_groups": ["scale_reset"]}},
      
      "attack0":{"add": {"component_groups": [ "attack0" ]}},
      "attack1":{"add": {"component_groups": [ "attack1" ]}},
      "movement0":{"add": {"component_groups": [ "movement0" ]}},
      "movement1":{"add": {"component_groups": [ "movement1" ]}},
      "knockback_resistance0":{"add": {"component_groups": [ "knockback_resistance0" ]}},
      "knockback_resistance100":{"add": {"component_groups": [ "knockback_resistance100" ]}},
      "scale0":{"add": {"component_groups": [ "scale0" ]}},
      "scale100":{"add": {"component_groups": [ "scale100" ]}},

      "minecraft:gain_bad_omen": { "add": { "component_groups": [ "minecraft:add_bad_omen" ] } },
      "minecraft:clear_add_bad_omen": { "remove": { "component_groups": [ "minecraft:add_bad_omen" ] }, "add": { "component_groups": [ "minecraft:clear_bad_omen_spell_effect" ] } },
      "minecraft:trigger_raid": { "add": { "component_groups": [ "minecraft:raid_trigger" ] } },
      "minecraft:remove_raid_trigger": { "remove": { "component_groups": [ "minecraft:raid_trigger" ] } }
    }
  }
}